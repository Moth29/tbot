{
    "version": 2,
    "builds": [
        { 
            "src": "api/webhook.py", 
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12",
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        { 
            "src": "/api/webhook", 
            "dest": "api/webhook.py" 
        }
    ]
}